{
  "name": "sideshift-api",
  "version": "1.0.9",
  "description": "A TypeScript REST API client library for interacting with Sideshift.ai API endpoints",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "build": "rollup -c",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run clean && npm run build",
    "dev": "nodemon --exec ts-node src/index.ts"
  },
  "keywords": [
    "sideshift",
    "sideshift.ai",
    "api",
    "crypto",
    "shift",
    "swap",
    "exchange",
    "typescript",
    "nodejs"
  ],
  "author": "Ryo Ohki Code",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ryo-ohki-code/sideshift-api-node.git"
  },
  "bugs": {
    "url": "https://github.com/ryo-ohki-code/sideshift-api-node/issues"
  },
  "homepage": "https://github.com/ryo-ohki-code/sideshift-api-node#readme",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^29.0.0",
    "@rollup/plugin-node-resolve": "^16.0.3",
    "@rollup/plugin-typescript": "^12.3.0",
    "@types/node": "^24.10.1",
    "rollup": "^4.53.2",
    "tslib": "^2.8.1"
  }
}
